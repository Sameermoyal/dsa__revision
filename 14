//14.Write a program to generate a new array where each element is the result of subtracting 2 from the original element.
#include<bits/stdc++.h>
using namespace std;

int main(){
   int arr[5]={2,3,4,5,66};
 
   vector<int>result(5);
   
   for(int i=0;i<sizeof(arr)/sizeof(arr[0]);i++){
      result[i]=arr[i]-2;
   }
   
   for(int i=0;i<result.size();i++){
      cout<<result[i]<<" ";
   }
    
}