//13.Create a program to store in a new array the modulo 5 of each element from the original array.

#include<bits/stdc++.h>
using namespace std;

int main(){
   int arr[5]={2,3,4,5,66};
 
   vector<int>result(5);
   
   for(int i=0;i<sizeof(arr)/sizeof(arr[0]);i++){
      result[i]=(arr[i]%5);
   }
   
   for(int i=0;i<result.size();i++){
      cout<<result[i]<<" ";
   }
    
}