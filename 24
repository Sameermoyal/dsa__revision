#include<bits/stdc++.h>
using namespace std;


// Write a program to add the current element of an array to the element present at the next index.
int main(){
  int arr[]={5,2,6,6,5,2,2};

   vector<int>newArr; 

   int n=sizeof(arr)/sizeof(arr[0]);
   
  for(int i=0;i<n-1;i++){
   arr[i]=arr[i]+arr[i+1];
  }
  
  for(auto it:arr){
      cout<<it<<endl;
  }
 
  
}