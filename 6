#include<bits/stdc++.h>
using namespace std;

//6.Write a program that multiplies each element by 10 and stores the result in a new array.


int main(){
    int arr[6]{1,4,8,10,45,12};
    int n=sizeof(arr)/sizeof(arr[0]);
    int newArr[n];
    
    for(int i=0;i<n;i++){
      newArr[i]=arr[i]*10;
    }
    
    for(auto it:newArr){
        cout<<it<<" ";
    }
   
}