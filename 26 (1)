#include<bits/stdc++.h>
using namespace std;


// Write a program to multiply each element of an array with its next element.
int main(){
  int arr[]={5,2,6,6,5,2,2};

   vector<int>newArr; 

   int n=sizeof(arr)/sizeof(arr[0]);
   int d;
  for(int i=0;i<n-1;i++){
    d=arr[i]*arr[i+1];
   newArr.push_back(d);
  }
  
  for(auto it:newArr){
      cout<<it<<endl;
  }
 
  
}