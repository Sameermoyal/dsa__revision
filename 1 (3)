#include<bits/stdc++.h>
using namespace std;

// 1.Write a program to check if each element of an array is divisible 
// by 3 and store the result as true (divisible) or false (not divisible).

int main(){
    int arr[6]={3,2,7,45,6,7};
   
    int n=sizeof(arr)/sizeof(arr[0]);
     bool store[n];
   
    for(int i=0;i<n;i++){
        if(arr[i]%3==0){
          store[i]=true;
        }else{
            store[i]=false;
        }
    }
    
    for(int i=0;i<n;i++){
      cout<< arr[i]<<(store[i] ?  " visible ":" not-visible ")<<endl;
       
    }
    
}