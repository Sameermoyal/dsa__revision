#include<bits/stdc++.h>
using namespace std;


// : Write a program to square each element of an array and store it in a new array.
int main(){
  int arr[]={5,2,6,6,5,2,2};

   vector<int>newArr; 

   int n=sizeof(arr)/sizeof(arr[0]);
   int d;
  for(int i=0;i<n;i++){
    d=arr[i]*arr[i];
   newArr.push_back(d);
  }
  
  for(auto it:newArr){
      cout<<it<<endl;
  }
 
  
}